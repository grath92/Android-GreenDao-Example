<manifest package="com.gopal.daogeneratortest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.gopal.daogeneratortest.activity.MainListActivity"
            android:label="@string/to_do">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.gopal.daogeneratortest.activity.TaskDetailActivity"
            android:label="@string/task_detail">
        </activity>

    </application>

</manifest>
